[
  {
    "title": "Debian 8.8",
    "description": "Debian 8.8 image",
    "logo": "https://www.open-scap.org/wp-content/uploads/2015/07/debian.png",
    "image": "davask/d-debian:8.8",
    "ports": [
        "22/tcp"
    ],
    "env": [
        {
            "name": "DWL_LOCAL_LANG",
            "label": "Local lang [en_US:en]"
        },{
            "name": "DWL_LOCAL",
            "label": "Local [en_US.UTF-8]"
        },{
            "name": "DWL_SUDO_USER",
            "label": "Username [username]"
        },{
            "name": "DWL_SSH_ACCESS",
            "label": "Enable ssh access [false]"
        },{
            "name": "DWL_USER_ID",
            "label": "User id [1000]"
        },{
            "name": "DWL_USER_PASSWD",
            "label": "User password for ssh access [secret]"
        }
    ],
    "volumes": [
        "/home/host"
    ]
  }
]
